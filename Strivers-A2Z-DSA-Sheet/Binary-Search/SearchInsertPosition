class Solution {
    public int searchInsert(int[] nums, int target) {
        int left=0,right=nums.length-1,ans =-1,mid=0;
        while(left<=right){
            mid=(left+right)/2;
            if(nums[mid]<target){
                ans=mid;
                left=mid+1;
            }else{
                right=mid-1;
            }
        }
        return ans+1;
    }
}
